{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "coverageDirectory": "testcoverage",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/docs/",
    "/lib",
    "testcoverage",
    "testSetup"
  ],
  "collectCoverageFrom": [
    "**/*.{js,jsx,ts,tsx}",
    "!**/node_modules/**",
    "!**/vendor/**",
    "!**/__tests__/**",
    "!**/docs/**",
    "!**/lib/**",
    "!**/testcoverage/**"
  ],
  "setupFiles": [
    "./testSetup.ts"
  ],
  "testRunner": "jest-circus/runner"
}